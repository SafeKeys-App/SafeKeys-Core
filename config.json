{
  "VAULT_VERSION": "1.0.0",
  "SUPPORTED_VERSIONS": ["1.0.0"],
  "DEFAULT_VAULT_SETTINGS": {
    "passwordGenerator": {
      "length": 16,
      "includeUppercase": true,
      "includeLowercase": true,
      "includeNumbers": true,
      "includeSymbols": true,
      "excludeSimilar": true
    },
    "security": {
      "lockTimeout": 15,
      "requireMasterPasswordOnStart": true,
      "maxFailedAttempts": 5
    },
    "ui": {
      "theme": "auto",
      "language": "en",
      "showFavorites": true,
      "defaultView": "list"
    }
  }
}
